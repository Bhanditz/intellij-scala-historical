<idea-plugin>

  <extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory implementation="intellijhocon.lang.HoconFileTypeFactory"/>
    <annotator language="Scala" implementationClass="intellijhocon.parser.HoconErrorHighlightingAnnotator"/>
    <lang.commenter language="Scala" implementationClass="intellijhocon.misc.HoconCommenter"/>
  </extensions>

</idea-plugin>
